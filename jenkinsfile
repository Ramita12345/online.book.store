pipeline {
    agent any

    stages {
        stage("git_checkout") {
            steps {
                echo "Cloning repository"
                // Add your git checkout step here
                checkout scm
                echo "Repository cloned successfully"
            }
        }

        stage("build") {
            steps {
                echo "Building Maven project"
                sh "mvn clean package"  // Maven build command
            }
        }

        stage("deploy") {
            steps {
                echo "Deploying artifact to Nexus"
                 withCredentials([usernamePassword(credentialsId: 'nexus_artifact1', usernameVariable: 'admin', passwordVariable: 'Nexus@1112')]) {
                sh "mvn deploy -DskipTests=true -Dmaven.javadoc.skip=true"  // Maven deploy command
            }
        }
    }
}
}
